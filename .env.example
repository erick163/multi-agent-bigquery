# =============================================================================
# GCP Configuration (Required)
# =============================================================================

# Your GCP project ID
GOOGLE_CLOUD_PROJECT=your-project-id

# GCP region for Vertex AI
GOOGLE_CLOUD_LOCATION=us-central1

# GCS bucket for Vertex AI staging (must exist)
GOOGLE_CLOUD_STAGING_BUCKET=your-staging-bucket

# =============================================================================
# BigQuery Configuration (Required)
# =============================================================================

# BigQuery dataset containing billing data
BQ_DATASET_ID=detailed_billing

# Billing export table name (without project/dataset prefix)
BQ_BILLING_TABLE=gcp_billing_export_resource_v1_XXXXXX_XXXXXX_XXXXXX

# Cloud pricing export table name
BQ_PRICING_TABLE=cloud_pricing_export

# Customer lookup table name
BQ_CUSTOMERS_TABLE=gcp_customers

# =============================================================================
# Optional Configuration
# =============================================================================

# Gemini model to use (default: gemini-2.5-flash)
GEMINI_MODEL=gemini-2.5-flash

# Enable Vertex AI (set to 1 for Vertex AI, 0 for Gemini API)
GOOGLE_GENAI_USE_VERTEXAI=1
